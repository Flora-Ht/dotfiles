[user]
	# TO_CHANGE: Email
	email = flora.huot@epitech.eu
	# TO_CHANGE: Full name
	name = Flora Huot
[push]
	default = simple
[alias]
	lol = log --graph --decorate --pretty=oneline --abbrev-commit
	lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
	ls = ls-tree --name-only HEAD
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
[credential]
	helper = /usr/lib/git-core/git-credential-gnome-keyring
