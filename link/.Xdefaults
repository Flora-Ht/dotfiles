rxvt*font:			9x15bold
rxvt*background:		black
rxvt*foreground:		white
rxvt*visualBell:		true
rxvt*scrollBar:			false
rxvt*saveLines:			2000
rxvt*scrollTtyOutput:		false
rxvt*scrollTtyKeypress:		true
rxvt*scrollWithBuffer:		true
rxvt*color0:			#000000
rxvt*color1:			#9e1828
rxvt*color2:			#aece92
rxvt*color3:			#968a38
